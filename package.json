{
  "name": "kikosan",
  "version": "1.1.0",
  "description": "A starting template for forgescript",
  "main": ".src/index.js",
  "type": "commonjs",
  "scripts": {
    "up": "pnpm install && pnpm update",
    "start": "clear && pnpm run lint && pnpm run format && clear && node ./src/index.js",
    "lint": "eslint --fix '**/*.{js,mjs,cjs,ts}'",
    "format": "prettier --write '**/*.{js,mjs,cjs,ts,json,css,html}'",
    "lint-and-format": "pnpm run lint && pnpm run format",
    "stable-release": "git checkout stable && git push origin stable --force && git checkout dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Zack-911/Atomic.git"
  },
  "keywords": [
    "discord",
    "bot",
    "opensource"
  ],
  "author": "Zack-911",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Zack-911/Atomic/issues"
  },
  "homepage": "https://github.com/Zack-911/Atomic#readme",
  "dependencies": {
    "@tryforge/forge.canvas": "^1.1.1",
    "@tryforge/forge.db": "^2.0.7",
    "@tryforge/forgescript": "github:tryforge/forgescript#dev",
    "chalk": "^4.1.2",
    "figlet": "^1.8.0",
    "path": "^0.12.7",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "dotenv": "^16.4.7",
    "eslint": "^9.17.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.14.0",
    "prettier": "^3.4.2",
    "typescript-eslint": "^8.18.2"
  }
}
